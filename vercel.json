
{
    "version": 2,
    "builds": [
      {
        "src": "sitewsgi/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb",
        "runtime": "python3.10",
        "buildCommand": "bash build_files.sh"
       }  
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "sitewsgi/wsgi.py"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      }
    ]
  }